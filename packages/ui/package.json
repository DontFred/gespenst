{
  "name": "@gespenst/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./avatar": {
      "types": "./src/avatar/index.tsx",
      "import": "./dist/avatar/index.mjs",
      "require": "./dist/avatar/index.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./font-geist": {
      "types": "./src/font-geist/index.tsx",
      "import": "./dist/font-geist/index.mjs",
      "require": "./dist/font-geist/index.js"
    },
    "./text": {
      "types": "./src/text/index.tsx",
      "import": "./dist/text/index.mjs",
      "require": "./dist/text/index.js"
    },
    "./button": {
      "types": "./src/button/index.tsx",
      "import": "./dist/button/index.mjs",
      "require": "./dist/button/index.js"
    },
    "./lib": {
      "types": "./src/lib/index.ts",
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:icons": "npx tsx ./src/icon/build.mts",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fix:nowarnings": "eslint . --fix --max-warnings 0",
    "clean:all": "rm -rf dist"
  },
  "devDependencies": {
    "@gespenst/eslint-config": "*",
    "@gespenst/typescript-config": "*",
    "@types/eslint": "^8.56.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "loading-cli": "^1.1.2",
    "node-html-parser": "^6.1.13",
    "react": "^18.2.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "geist": "^1.3.0",
    "tailwind-merge": "^2.3.0",
    "tsup": "^8.0.2"
  }
}
